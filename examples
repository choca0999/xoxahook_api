cheat.set_event_callback("draw", function()
	local x, y = engine.get_screen_size()
	render.text(x / 2, y / 2, Color.new(255, 255, 255, 255), "prediction.lua")
end)

cheat.set_event_callback("createmove", function()
	local local_index = engine.GetLocalPlayer()
	local local_player = entlist.get_client_entity(local_index)
	if (local_player == nil) then
		return 
	end
end)
